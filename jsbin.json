.hamburger-button {
  position: fixed;
  right: 0;
  z-index: 999;
  width: 3.125rem;
  height: 3.125rem;
  font-size: 1.75rem;
  color: #fff;
}

/* ハンバーガーメニュー */
.hamburger-menu {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(4, 0, 0, 0.9);
  transition: opacity .3s linear;
  opacity: 0;
}

/* スライドイン後の位置の指定 */
.hamburger-menu-active {
  opacity: 1;
}

.hamburger-menu-list {
  padding: 5rem 1.25rem;
  list-style: none;
}


/**
